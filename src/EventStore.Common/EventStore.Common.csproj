<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\version.props" />
  <Import Project="..\..\nuget.props" />
  <Import Project="..\..\..\CuteAnt.Extensions.Common\src\CuteAnt.Extensions.Logging.Sources\CuteAnt.Extensions.Logging.Sources.projitems" Label="Shared" />
  
  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.2;net471</TargetFrameworks>
    <RootNamespace>EventStore.Common</RootNamespace>
    <AssemblyName>EventStore.Common</AssemblyName>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Label="NuGet">
    <PackageId>EventStore.Common.Unofficial</PackageId>
    <Title>EventStore.Common.Unofficial</Title>
    <Description>EventStore.Common.Unofficial</Description>
    <PackageTags>eventstore</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\EventStore.ClientAPI.Abstractions\Common\MatchHandler\HandlerKind.cs" Link="MatchHandler\HandlerKind.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\Argument.cs" Link="MatchHandler\Argument.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\CachedMatchCompiler.cs" Link="MatchHandler\CachedMatchCompiler.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\CompiledMatchHandlerWithArguments.cs" Link="MatchHandler\CompiledMatchHandlerWithArguments.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\ILambdaExpressionCompiler.cs" Link="MatchHandler\ILambdaExpressionCompiler.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\IMatchCompiler.cs" Link="MatchHandler\IMatchCompiler.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\IMatchExpressionBuilder.cs" Link="MatchHandler\IMatchExpressionBuilder.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\IPartialActionBuilder.cs" Link="MatchHandler\IPartialActionBuilder.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\LambdaExpressionCompiler.cs" Link="MatchHandler\LambdaExpressionCompiler.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\MatchBuilder.cs" Link="MatchHandler\MatchBuilder.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\MatchBuilderSignature.cs" Link="MatchHandler\MatchBuilderSignature.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\MatchExpressionBuilder.cs" Link="MatchHandler\MatchExpressionBuilder.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\MatchExpressionBuilderResult.cs" Link="MatchHandler\MatchExpressionBuilderResult.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\PartialAction.cs" Link="MatchHandler\PartialAction.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\PartialActionBuilder.cs" Link="MatchHandler\PartialActionBuilder.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\PartialHandlerArgumentsCapture.cs" Link="MatchHandler\PartialHandlerArgumentsCapture.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\PredicateAndHandler.cs" Link="MatchHandler\PredicateAndHandler.cs" />
    <Compile Include="..\EventStore.ClientAPI\Common\MatchHandler\TypeHandler.cs" Link="MatchHandler\TypeHandler.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CuteAnt.Core" Version="$(CuteAntCorePackageVersion)" />
    <PackageReference Include="CuteAnt.Extensions.Serialization" Version="$(CuteAntExtensionsSerializationPackageVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\EventStore.Common.Utils\EventStore.Common.Utils.csproj" />
    <ProjectReference Include="..\EventStore.Rags\EventStore.Rags.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Internal\Strings.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Strings.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Internal\Strings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Strings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
</Project>
