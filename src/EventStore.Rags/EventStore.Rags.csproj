<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="..\..\version.props" />
  <Import Project="..\..\nuget.props" />

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.2;net471</TargetFrameworks>
    <RootNamespace>EventStore.Rags</RootNamespace>
    <AssemblyName>EventStore.Rags</AssemblyName>
    <AllowUnsafeBlocks>false</AllowUnsafeBlocks>
  </PropertyGroup>

  <PropertyGroup Label="NuGet">
    <PackageId>EventStore.Rags.Unofficial</PackageId>
    <Title>EventStore.Rags.Unofficial</Title>
    <Description>EventStore.Core.Unofficial</Description>
    <PackageTags>eventstore</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="YamlDotNet.Signed" Version="$(YamlDotNetSignedPackageVersion)" />
  </ItemGroup>

</Project>
